{
  "name": "कोषFLOW-cloud-finance",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"Frontend,Backend\" --prefix-colors \"cyan,yellow\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run lint && npm run type-check && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"No tests specified yet\"",
    "clean": "rm -rf dist node_modules/.vite backend/node_modules/.cache",
    "reset": "npm run clean && npm install && cd backend && npm install",
    "server": "node backend/src/server.js",
    "server:dev": "nodemon backend/src/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node backend/src/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "health": "curl -f http://localhost:3001/health || echo 'Backend not running'",
    "start": "npm run build && npm run preview"
  },
